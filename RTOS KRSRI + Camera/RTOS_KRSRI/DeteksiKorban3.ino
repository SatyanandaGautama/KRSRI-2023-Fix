void command(String message = "beri message", int choice_ = 0, bool act_queue = false){
  DEBUG_SERIAL.println(message);
  if (act_queue) {
    queue = true;
    send_queue = choice_;
    xQueueSend(queueHandle, &send_queue, 10);
    //    DEBUG_SERIAL.print("sebelum kirim Queue : ");
    //    DEBUG_SERIAL.println(choice);
  }
  else {
    choice_sem = choice_;
    xSemaphoreGive(semHandle);
    //xSemaphoreGive(mutex);
    //    DEBUG_SERIAL.print("sebelum kirim Semaphore : ");
    //    DEBUG_SERIAL.println(choice);
  }
}
