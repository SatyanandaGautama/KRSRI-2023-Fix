void ambilK1() {
  float deg1 = 225; //205
  float deg2 = 160; //190
  float xFront = 53;
  float yFront = 57;
  float xBack = 58;
  for (int i = 0; i <= 49; i++) {
    FR(xFront, yFront, 0, 0);
    BR(xBack, -55, 0, 0);
    BL3(-(xBack), -55, 0, 0);
    FL(-(xFront), yFront, 0, 0);
    dxl.setGoalPosition(14, deg1, UNIT_DEGREE);
    dxl.setGoalPosition(15, deg2, UNIT_DEGREE);
    deg1 -= 0.4;
    deg2 += 0.6;
    xFront -= 1.0252; //tujuan = 1.74
    yFront += 0.9282; //tujuan = 103.41
    xBack += 1.14; // tujuan = 115
    delay(15);
  }
}
